\mode<presentation>
\ProcessOptionsBeamer

\definecolor{STATred}{rgb}{0.694,0.0,0.149}
\definecolor{STATgrey}{rgb}{0.8,0.8,0.8}
\definecolor{STATdarkgrey}{rgb}{0.3,0.3,0.3}
\definecolor{STATdarkblue}{rgb}{0.22,0.38,0.57}
\definecolor{STATblue}{rgb}{0.41,0.53,0.68}
\definecolor{STATlightblue1}{rgb}{0.58,0.66,0.77}
\definecolor{STATlightblue3}{rgb}{0.33,0.55,0.84}
\definecolor{STATlightblue3}{rgb}{0.73,0.80,0.90}

\definecolor{headercolor}{RGB}{32,74,135}
\definecolor{canvascolor}{RGB}{164,0,0}

\definecolor{da_blue}{RGB}{32,74,135}
\definecolor{da_red}{RGB}{164,0,0}
\definecolor{da_green}{RGB}{78,154,6}
\definecolor{da_grey}{RGB}{136,138,133}

\setbeamercolor*{normal text}{fg=black}
\setbeamercolor*{alerted text}{fg=STATred}
\setbeamercolor*{example text}{fg=STATgrey}

\setbeamercolor*{structure}{fg=STATred}
\setbeamercolor*{titlelike}{fg=STATred}

\setbeamercolor{palette primary}{use=structure,fg=black}
\setbeamercolor{palette secondary}{use=structure,fg=black}
\setbeamercolor{palette tertiary}{use=structure,fg=black}
\setbeamercolor{palette quaternary}{use=structure,fg=black}

\setbeamercolor{palette sidebar primary}{use=normal text,fg=black}
\setbeamercolor{palette sidebar secondary}{use=structure,fg=black}
\setbeamercolor{palette sidebar tertiary}{use=normal text,fg=black}
\setbeamercolor{palette sidebar quaternary}{use=structure,fg=black}

% Header-Bereich
\setbeamercolor*{frametitle}{use=headline,fg=STATred,bg=white}

% Sidebar
\setbeamercolor*{sidebar}{bg=white}

% untere und obere trennlinien
\setbeamercolor*{upper separation line foot}{fg=STATlightblue3}
\setbeamercolor*{foot pagenumber}{fg=STATred}
\setbeamercolor*{author date institut}{fg=STATdarkgrey}
\setbeamercolor*{lower separation line head}{fg=STATlightblue3}

\mode<all>
